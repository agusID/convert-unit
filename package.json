{
  "name": "css-convert-units",
  "version": "1.0.0",
  "description": "CSS Convert Units is a library for convert css unit to another unit.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "flow": "flow",
    "flow:check": "flow check ./public/src/",
    "flow:build": "flow-remove-types ./public/src/lib/convert-unit/flow-typed -d ./public/src/lib/convert-unit/ --all --pretty",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agusID/convert-unit-library.git"
  },
  "keywords": [
    "converter",
    "convert",
    "css",
    "unit"
  ],
  "author": "Dwi Agustianto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agusID/convert-unit-library/issues"
  },
  "homepage": "https://github.com/agusID/convert-unit-library#readme",
  "devDependencies": {
    "flow-bin": "^0.103.0",
    "flow-remove-types": "^2.103.0"
  },
  "dependencies": {
    "module-alias": "^2.2.0"
  },
  "_moduleAliases": {
    "@lib": "public/src/lib"
  }
}
